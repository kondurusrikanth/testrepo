

CREATE COMPUTE MODULE test3_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
	
		 set OutputRoot.Properties.CodedCharSetId='5348gg';
		
		
		
        DECLARE a CHARACTER 'hi';
      
      SET OutputRoot.BLOB.BLOB =  BASE64ENCODE(a);
  
	 	PROPAGATE TO TERMINAL 'out1' DELETE NONE;
		
		SET OutputRoot=NULL;
		SET OutputRoot.XMLNSC.status='file created';
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
